{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DBConnectionString": "Server=localhost,1434;Database=InfoSafeDB;User Id=sa;Password=Admin1234;TrustServerCertificate=True",
    "RMQConnectionString": "localhost",
    "ApplicationInsightsConnectionString": "",
    "AppConfigConnectionString": ""
  },
  "ClientUri": "http://localhost:4200",
  "AzureAD": {
    "Authority": "FromSecret",
    "Audience": "FromSecret",
    "ValidIssuer": "FromSecret"
  },
  "Stripe": {
    "ApiKey": "FromSecret"
  },
  "FeatureManagement": {
    "FeatureA": true,
    "FeatureB": false,
    "FeatureC": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "FeatureD": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "2023-10-01T00:00:00+00:00",
            "End": "2023-10-31T00:00:00+00:00"
          }
        }
      ]
    },
    "FeatureE": {
      "EnabledFor": [
        {
          "Name": "Random",
          "Parameters": {
            "Method": "Even"
          }
        }
      ]
    }
  }
}